#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/yanjieyan/workSpace/vuepressTest/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_4978286b0e34698f26d875cd57f1f993/node_modules/vuepress/bin/node_modules:/mnt/d/yanjieyan/workSpace/vuepressTest/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_4978286b0e34698f26d875cd57f1f993/node_modules/vuepress/node_modules:/mnt/d/yanjieyan/workSpace/vuepressTest/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_4978286b0e34698f26d875cd57f1f993/node_modules:/mnt/d/yanjieyan/workSpace/vuepressTest/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/yanjieyan/workSpace/vuepressTest/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_4978286b0e34698f26d875cd57f1f993/node_modules/vuepress/bin/node_modules:/mnt/d/yanjieyan/workSpace/vuepressTest/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_4978286b0e34698f26d875cd57f1f993/node_modules/vuepress/node_modules:/mnt/d/yanjieyan/workSpace/vuepressTest/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_4978286b0e34698f26d875cd57f1f993/node_modules:/mnt/d/yanjieyan/workSpace/vuepressTest/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vuepress@2.0.0-rc.20_@vuepr_4978286b0e34698f26d875cd57f1f993/node_modules/vuepress/bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vuepress@2.0.0-rc.20_@vuepr_4978286b0e34698f26d875cd57f1f993/node_modules/vuepress/bin/vuepress-vite.js" "$@"
fi
